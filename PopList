my_list = [9,2,3,4,6,8,7,5,1]
print(my_list)
my_list.sort()
print(my_list)
my_list = ["H", "e", "l", "l", "o", " ", "W", "o", "r", "l", "d",]
print(my_list)
# Using the sort() method to sort the list
my_list.sort()
print(my_list)
# pop Clown
my_list = ["STEAM", "Clown", "Productions"]
print(my_list)
x = my_list.pop(1)
print(my_list)
print(x)
my_list[-1] = x
print(my_list)
my_list.append('Inc')
print(my_list)
my_list.pop(-1)
print(my_list)
